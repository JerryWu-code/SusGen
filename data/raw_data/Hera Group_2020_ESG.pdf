      function primeterzeparti() {
  $(".btnaccettacookie").addClass("active");
  $(".btnterzeparti").removeClass("active");
  $(".cc_b_ok").click();
  setCookiefunction("cookie-profilazione", "true", "182");
  let recuperocookiaccettatie = getCookiefunction("cookie-profilazione");
  if (recuperocookiaccettatie == "true") {
    $(".coockieinput").each(function (index, element) {
      let srcelement = $(element).val();
      $(element).parent().find(".coockieinput-iframe").attr("src", srcelement);
    });
  }
}

function setCookiefunction(cname, cvalue, exdays) {
  console.log("sono stato richiamato");
  var d = new Date();
  d.setTime(d.getTime() + exdays * 24 * 60 * 60 * 1000);
  var expires = "expires=" + d.toUTCString();
  document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}

function getCookiefunction(cname) {
  var name = cname + "=";
  var decodedCookie = decodeURIComponent(document.cookie);
  var ca = decodedCookie.split(";");
  for (var i = 0; i < ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == " ") {
      c = c.substring(1);
    }
    if (c.indexOf(name) == 0) {
      return c.substring(name.length, c.length);
    }
  }
  return "";
}

function terzeparti() {
  removeAllCookies();
  setCookiefunction("cookie-profilazione", "true", "0");
  $(".cc_dialog").addClass("hidden");
  $(".btnterzeparti").addClass("active");
  $(".btnaccettacookie").removeClass("active");
  setCookiefunction("cookie-profilazione", "false", "182");
}

function removeAllCookies() {
  var cookies = document.cookie.split(";");
  for (var i = 0; i < cookies.length; i++) {
    document.cookie = cookies[i] + "=; expires=Thu, 01 Jan 1970 00:00:00 GMT; path=/;domain=.gruppohera.it";
  }
}
    function checkCookie() {
        var cookieValue = getCookie("cookie-profilazione");
        
        if (cookieValue === "true") {
            // Fai qualcosa se il cookie  settato a true
            primeterzeparti()
        } else if (cookieValue === "false") {
            // Fai qualcosa se il cookie  settato a false
            terzeparti()
        } else {
            // Fai qualcosa se il cookie non esiste
            return
        }
    }

    function getCookie(name) {
        var value = "; " + document.cookie;
        var parts = value.split("; " + name + "=");
        if (parts.length === 2) {
            return parts.pop().split(";").shift();
        }
    }

    checkCookie();
});
</script>






						
					
				
			
		
	



